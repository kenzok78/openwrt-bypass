
config defaults 'defaults'
	option enabled '1'
	option timeout '2'
	option count '2'
	option tries '3'
	option interval '2'
	option interval_tries '1'
	option type 'ping'
	option wait_test '0'
	option server_http_test '0'
	option server_test '0'
	option restart_down '0'
	option mail_alert '0'
	option initial_state 'online'
	option family 'ipv4'
	option reliability '1'
	option failure_interval '20'
	option tries_up '5'

config proxy 'proxy'
	option enabled '1'
	option country 'world'
	option timeout '10'
	option tries '3'
	option wait_test '0'
	option interval_tries '1'
	option interval '10'
	option mail_alert '0'
	option initial_state 'online'
	option family 'ipv4ipv6'

config server 'server'
	option enabled '1'
	option tries '3'
	option timeout '10'
	option wait_test '0'
	option interval '5'
	option mail_alert '0'
	option initial_state 'online'
	option type 'apiping'

config interface 'omrvpn'
	option type 'none'
	option timeout '10'
	option tries '2'
	option interval '5'
	option mail_alert '0'
	option enabled '1'
	option server_http_test '1'
	option restart_down '0'
	list hosts '4.2.2.1'
	list hosts '8.8.8.8'
	option initial_state 'online'
	option family 'ipv4'
	option reliability '1'
	option count '1'
	option failure_interval '3'
	option tries_up '1'

